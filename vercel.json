{
  "version": 2,
  "framework": null,
  "buildCommand": "cd frontend && npm install && npm run build && cd ../admin && npm install && npm run build",
  "outputDirectory": "frontend/dist",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.js"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/admin/dist/$1"
    },
    {
      "src": "/admin",
      "dest": "/admin/dist/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/dist/$1"
    }
  ]
}
